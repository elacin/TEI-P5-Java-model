    <!--<jxb:typeName suffix="Type"/>-->
    <!--<jxb:anonymousTypeName suffix="A"/>-->

    <!--</jxb:nameXmlTransform>-->

    <!--</jxb:schemaBindings>-->
    <!--</jxb:bindings>-->

    <jxb:globalBindings>
        <xjc:simple/>
    </jxb:globalBindings>

</jxb:bindings>


